<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    android:orientation="vertical"
    tools:context=".mainStuff.MainActivity">




    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/relativeLayout"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/ic_circle"
        ads:layout_constraintBottom_toTopOf="@+id/linearLayout6"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintHorizontal_bias="0.51"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/allTotal"
            style="@style/MainTextViews"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginBottom="24dp"
            android:text="المجموع العام"
            android:textAlignment="center"
            android:textSize="16sp"
            ads:layout_constraintBottom_toTopOf="@+id/TotalallOut"
            ads:layout_constraintEnd_toEndOf="@+id/TotalallOut"
            ads:layout_constraintStart_toStartOf="@+id/TotalallOut"
            ads:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/TotalallOut"
            style="@style/SumTextViews"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="2dp"
            android:text="12000"
            ads:autoSizeTextType="uniform"
            ads:layout_constraintBottom_toBottomOf="parent"
            ads:layout_constraintEnd_toEndOf="parent"
            ads:layout_constraintStart_toStartOf="parent"
            ads:layout_constraintTop_toBottomOf="@+id/allTotal"
            tools:maxLines="1" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        style="@style/MainContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:orientation="vertical"
        ads:layout_constraintBottom_toTopOf="@+id/linearLayout"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintHorizontal_bias="0.0"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/relativeLayout">


        <TextView
            android:id="@+id/textView"
            style="@style/DetailTextViews"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="المتبقي من السقف المحدد حسب الوضع عائلي / ضيوف "
            ads:layout_constraintEnd_toEndOf="parent"
            ads:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/QuotaLeftOut"
            style="@style/NumbersTextViews"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:text="195"
            ads:layout_constraintBottom_toBottomOf="@+id/textView"
            ads:layout_constraintTop_toTopOf="@+id/textView"
            ads:layout_constraintVertical_bias="0.555"
            tools:layout_editor_absoluteX="55dp" />

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:progress="30"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="309dp" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout"
        style="@style/MainContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:orientation="vertical"
        ads:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintHorizontal_bias="0.0"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/linearLayout6">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    style="@style/SpinnerContainer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/justborderz"
                    android:layout_margin="4dp">

                    <Spinner
                        android:id="@+id/SumCategory"
                        style="@style/Spinners"
                        android:layout_width="0dp"
                        android:layout_height="40sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    style="@style/SpinnerContainer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/justborderz"

                    android:layout_margin="4dp">

                    <Spinner
                        android:id="@+id/SumSearch"
                        style="@style/Spinners"
                        android:layout_width="0dp"
                        android:layout_height="40sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    style="@style/SpinnerContainer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/justborderz"

                    android:layout_margin="4dp">

                    <Spinner
                        android:id="@+id/SumsearchBydate"
                        style="@style/Spinners"
                        android:layout_width="0dp"
                        android:layout_height="40sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <TextView
                    android:id="@+id/TotalTodayOut"
                    style="@style/SumTextViews"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="..."
                    android:textColor="@color/colorAccent" />
            </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:orientation="horizontal"
        ads:layout_constraintBottom_toBottomOf="parent"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <com.google.android.gms.ads.AdView
            android:id="@+id/admain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_margin="4dp"
            ads:adSize="BANNER"
            ads:adUnitId="ca-app-pub-9838187586217611/1394031397" />
    </LinearLayout>

    <Button
        android:id="@+id/fab"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_marginStart="86dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="22dp"
        android:background="@drawable/ic_sbasketadd"
        ads:layout_constraintBottom_toTopOf="@+id/linearLayout6"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toEndOf="@+id/relativeLayout"
        ads:layout_constraintTop_toBottomOf="@+id/linearLayout7" />

    <Button
        android:id="@+id/menubutton"
        android:layout_width="29dp"
        android:layout_height="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/ic_baseline_menu_24"
        android:onClick="showPopup"
        ads:layout_constraintBottom_toTopOf="@+id/linearLayout7"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintHorizontal_bias="0.979"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout7"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_ttetl"
        android:orientation="vertical"
        ads:layout_constraintBottom_toBottomOf="@+id/relativeLayout"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toEndOf="@+id/relativeLayout"
        ads:layout_constraintTop_toTopOf="@+id/relativeLayout">

        <Switch
            android:id="@+id/SwitchGuest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:maxLines="1"
            android:text="@string/guest_mode"
            android:textAlignment="center"
            android:textColor="@color/Text_details"
            android:textSize="18sp"
            ads:layout_constraintLeft_toLeftOf="parent"
            ads:layout_constraintRight_toRightOf="parent"
            ads:layout_constraintTop_toTopOf="parent"
            tools:layout_gravity="end" />

        <TextView
            android:id="@+id/hereisurqt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:autoSizeMaxTextSize="16sp"
            android:autoSizeTextType="uniform"
            android:text="@string/_20000"
            android:textAlignment="center"
            android:textColor="@color/Text"
            ads:layout_constraintBottom_toBottomOf="parent"
            ads:layout_constraintLeft_toLeftOf="parent"
            ads:layout_constraintRight_toRightOf="parent"
            ads:layout_constraintTop_toBottomOf="@+id/SwitchGuest"
            tools:targetApi="o" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_ttetr"
        android:orientation="horizontal"
        ads:layout_constraintBottom_toBottomOf="@+id/relativeLayout"
        ads:layout_constraintEnd_toStartOf="@+id/relativeLayout"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="@+id/relativeLayout">

        <TextView
            android:id="@+id/dateView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:padding="2dp"
            android:text="@string/_12_01_20"
            android:textAlignment="gravity"
            android:textColor="@color/Text_details"
            ads:layout_constraintBottom_toBottomOf="parent"
            ads:layout_constraintEnd_toEndOf="parent"
            ads:layout_constraintStart_toStartOf="parent"
            ads:layout_constraintTop_toTopOf="parent"
            tools:layout_conversion_absoluteHeight="23dp"
            tools:layout_conversion_absoluteWidth="76dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ads:barrierDirection="left" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ads:barrierDirection="left" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ads:barrierDirection="top" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>
